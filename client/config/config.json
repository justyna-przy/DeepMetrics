{
    "logging_config": {
        "console_output": {
            "enabled": true,
            "level": "DEBUG",
            "format": "%(log_color)s%(levelname).1s:[%(name)s](%(asctime)s)> %(message)s",
            "date_format": "%Y-%m-%d %H:%M:%S",
            "log_colors": {
                "DEBUG": "cyan",
                "INFO": "green",
                "WARNING": "yellow",
                "ERROR": "red",
                "CRITICAL": "red,bg_white"
            }
        },
        "file_output": {
            "enabled": true,
            "level": "WARNING",
            "format": "%(asctime)s -%(levelname)s - %(name)s -  %(pathname)s:%(lineno)d - %(message)s",
            "date_format": "%Y-%m-%d %H:%M:%S",
            "log_dir": "logs",
            "filename": "app.log",
            "max_bytes": 10485760,
            "backup_count": 5
        }
    },
    "device_config": {
        "huggingface": {
            "base_url": "https://huggingface.co",
            "endpoint": "/api/models",
            "params": {
                "sort": "downloads",
                "direction": "-1", 
                "limit": 10
            },
            "num_models": 5
        }
    }
}